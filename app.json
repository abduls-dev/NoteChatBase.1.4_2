{
  "expo": {
    "name": "NoteChat",
    "slug": "NoteChatBase",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/Icon01.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/Splash01.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/Icon01.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.themirza009.NoteChatBase"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "5946efa7-69d6-4e2a-8416-9e06fb7e908f"
      }
    }
  }
}
